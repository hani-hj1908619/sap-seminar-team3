{
  "welcomeFile": "/cp.portal",
  "authenticationMethod": "route",
  "sessionTimeout": 30,
  "logout": {
    "logoutEndpoint": "/do/logout",
    "logoutPage": "/"
  },
  "routes": [
    {
      "source": "^/api/(.*)$",
      "target": "$1",
      "destination": "srv-api",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/vrp-analytics/(.*)$",
      "destination": "srv-api",
      "authenticationType": "xsuaa"
    }
  ]
}